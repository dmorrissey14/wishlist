<!-- app/views/home/sign_up.html.erb -->
<div class="mainContent new-list">
  <div class="grid">
  <div class="col-md-6 col-md-offset-3">
   <h1 class="largeHeader">Create New List:</h1>
    <%= form_for(List.new) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.label :description %>
      <%= f.text_field :description, class: 'form-control' %>
      <%= f.submit "Create List", class: "btn btn-primary" %>
    <% end %>

    </div><!-- end column 2 contact form -->
  </div><!-- end grid -->
</div><!-- end mainContent -->


