<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>

  <div class="col-md-8">
    <% if @user.list.any? %>
      <h3><%= @user.first_name %> <%= @user.last_name %>'s Lists</h3>
      <ol class="list">
        <%= render @list %>
      </ol>
    <% end %>
  </div>

  <div class="col_4">
    <input type="button" onclick="location.href='lists/createlist';" value="Create New List" />
  </div>
  
